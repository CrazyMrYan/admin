{"ast":null,"code":"import { defineComponent, ref, watch } from \"vue\";\nimport { Message } from \"@arco-design/web-vue\";\nimport NavMenu from \"@/components/nav-menu.vue\";\nimport { useRoute } from \"vue-router\";\nimport { IconCaretRight, IconCaretLeft } from \"@arco-design/web-vue/es/icon\";\nexport default defineComponent({\n  name: \"layout-page\",\n  components: {\n    NavMenu,\n    IconCaretRight,\n    IconCaretLeft\n  },\n\n  setup() {\n    const route = useRoute();\n    const collapsed = ref(false);\n    const hidden = ref(true);\n    watch(() => route.meta, val => hidden.value = val.hidden);\n\n    const onCollapse = () => {\n      collapsed.value = !collapsed.value;\n    };\n\n    return {\n      collapsed,\n      onCollapse,\n      hidden,\n\n      onClickMenuItem(key) {\n        Message.info({\n          content: `You select ${key}`,\n          showIcon: true\n        });\n      }\n\n    };\n  }\n\n});","map":{"version":3,"mappings":"AA8BA,SAASA,eAAT,EAA0BC,GAA1B,EAA+BC,KAA/B,QAA4C,KAA5C;AACA,SAASC,OAAT,QAAwB,sBAAxB;AACA,OAAOC,OAAP,MAAoB,2BAApB;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,SAASC,cAAT,EAAyBC,aAAzB,QAA8C,8BAA9C;AAEA,eAAeP,eAAe,CAAC;EAC7BQ,IAAI,EAAE,aADuB;EAE7BC,UAAU,EAAE;IACVL,OADU;IAEVE,cAFU;IAGVC;EAHU,CAFiB;;EAO7BG,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,EAAtB;IACA,MAAMO,SAAQ,GAAIX,GAAG,CAAC,KAAD,CAArB;IACA,MAAMY,MAAK,GAAIZ,GAAG,CAAC,IAAD,CAAlB;IAEAC,KAAK,CACH,MAAMS,KAAK,CAACG,IADT,EAEHC,GAAE,IAAKF,MAAM,CAACG,KAAP,GAAeD,GAAG,CAACF,MAFvB,CAAL;;IAMA,MAAMI,UAAS,GAAI,MAAM;MACvBL,SAAS,CAACI,KAAV,GAAkB,CAACJ,SAAS,CAACI,KAA7B;IACD,CAFD;;IAGA,OAAO;MACLJ,SADK;MAELK,UAFK;MAGLJ,MAHK;;MAILK,eAAe,CAACC,GAAD,EAAM;QACnBhB,OAAO,CAACiB,IAAR,CAAa;UAAEC,OAAO,EAAG,cAAaF,GAAI,EAA7B;UAAgCG,QAAQ,EAAE;QAA1C,CAAb;MACD;;IANI,CAAP;EAQD;;AA7B4B,CAAD,CAA9B","names":["defineComponent","ref","watch","Message","NavMenu","useRoute","IconCaretRight","IconCaretLeft","name","components","setup","route","collapsed","hidden","meta","val","value","onCollapse","onClickMenuItem","key","info","content","showIcon"],"sourceRoot":"","sources":["/www/wwwroot/admin/src/layout/index.vue"],"sourcesContent":["<template>\n  <a-layout v-if=\"!hidden\" class=\"layout\">\n    <a-layout-sider hide-trigger collapsible :collapsed=\"collapsed\">\n      <div class=\"logo\" />\n      <NavMenu />\n    </a-layout-sider>\n    <a-layout>\n      <a-layout-header style=\"padding-left: 20px\">\n        <a-button shape=\"round\" @click=\"onCollapse\">\n          <IconCaretRight v-if=\"collapsed\" />\n          <IconCaretLeft v-else />\n        </a-button>\n      </a-layout-header>\n      <a-layout style=\"padding: 0 24px\">\n        <a-breadcrumb :style=\"{ margin: '16px 0' }\">\n          <a-breadcrumb-item>Home</a-breadcrumb-item>\n          <a-breadcrumb-item>List</a-breadcrumb-item>\n          <a-breadcrumb-item>App</a-breadcrumb-item>\n        </a-breadcrumb>\n        <a-layout-content class=\"container\">\n          <router-view />\n        </a-layout-content>\n      </a-layout>\n    </a-layout>\n  </a-layout>\n  <div v-else>\n    <router-view />\n  </div>\n</template>\n<script>\nimport { defineComponent, ref, watch } from \"vue\";\nimport { Message } from \"@arco-design/web-vue\";\nimport NavMenu from \"@/components/nav-menu.vue\";\nimport { useRoute } from \"vue-router\";\nimport { IconCaretRight, IconCaretLeft } from \"@arco-design/web-vue/es/icon\";\n\nexport default defineComponent({\n  name: \"layout-page\",\n  components: {\n    NavMenu,\n    IconCaretRight,\n    IconCaretLeft\n  },\n  setup() {\n    const route = useRoute();\n    const collapsed = ref(false);\n    const hidden = ref(true);\n\n    watch(\n      () => route.meta,\n      val => hidden.value = val.hidden\n    );\n\n\n    const onCollapse = () => {\n      collapsed.value = !collapsed.value;\n    };\n    return {\n      collapsed,\n      onCollapse,\n      hidden,\n      onClickMenuItem(key) {\n        Message.info({ content: `You select ${key}`, showIcon: true });\n      },\n    };\n  },\n});\n</script>\n<style scoped>\n.container {\n  padding: 10px;\n  box-sizing: border-box;\n}\n.layout {\n  height: 100vh;\n  background: var(--color-fill-2);\n  border: 1px solid var(--color-border);\n  box-sizing: border-box;\n}\n.layout :deep(.arco-layout-sider) .logo {\n  height: 32px;\n  margin: 12px 8px;\n  background: rgba(255, 255, 255, 0.2);\n}\n.layout :deep(.arco-layout-sider-light) .logo {\n  background: var(--color-fill-2);\n}\n.layout :deep(.arco-layout-header) {\n  height: 64px;\n  line-height: 64px;\n  background: var(--color-bg-3);\n}\n.layout :deep(.arco-layout-footer) {\n  height: 48px;\n  color: var(--color-text-2);\n  font-weight: 400;\n  font-size: 14px;\n  line-height: 48px;\n}\n.layout :deep(.arco-layout-content) {\n  color: var(--color-text-2);\n  font-weight: 400;\n  font-size: 14px;\n  background: var(--color-bg-3);\n}\n.layout :deep(.arco-layout-footer),\n.layout :deep(.arco-layout-content) {\n  display: flex;\n  flex-direction: column;\n  color: var(--color-black);\n  font-size: 16px;\n  font-stretch: condensed;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}